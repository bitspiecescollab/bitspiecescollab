<decals>
  <decal path="GhostHouse/Lamp_">
    <light offsetY="-1" offsetX="-1" startFade="30" color="ffffff" alpha="0.6"/>
    <light offsetY="-1" offsetX="-1" startFade="10" color="ffffff" alpha="0.8"/>
  </decal>

  <decal path="GhostHouse/fireplace">
    <light offsetY="-1" offsetX="-1" startFade="50" color="ffffff" alpha="0.6"/>
    <light offsetY="10" offsetX="-1" startFade="20" color="ffffff" alpha="0.8"/>
  </decal>

  <decal path="GhostHouse/Summoning_Circle/CandleA">
    <light offsetY="-3" offsetX="-1" startFade="10" color="ff7066" alpha="0.5"/>
    <light offsetY="-3" offsetX="-1" startFade="5" color="ff7066" alpha="0.7"/>
  </decal>

  <decal path="GhostHouse/Vent_A">
